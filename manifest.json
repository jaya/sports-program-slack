{
  "display_information": {
    "name": "sports-program-slack"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": false,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "sports-program-slack",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/list-programs",
        "description": "List all programs"
      },
      {
        "command": "/list-activities",
        "description": "List all activities"
      },
      {
        "command": "/create-program",
        "description": "Create a new program"
      },
      {
        "command": "/signup",
        "description": "Sign up for a program"
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "channels:history",
        "chat:write",
        "im:history",
        "commands",
        "app_mentions:read",
        "channels:read",
        "groups:read",
        "mpim:read",
        "im:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": [
        "message.channels",
        "message.im",
        "app_mention"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}